h1.content-header(*ngIf="formDescriptor?.titleLabel") {{ formDescriptor?.titleLabel | translate }}

form.form((ngSubmit)="onSubmit()", [formGroup]="formInstance?.form", *ngIf='formInstance?.form')
  div(*ngFor="let field of formInstance?.fields")
    gtrack-form-field([field]="field", [form]="formInstance?.form", [submit]='getOnSubmit()')

  p-button(*ngIf="formDescriptor.submit?.translatableLabel", [ngClass]="formDescriptor.submit?.classList", [label]="formDescriptor.submit?.translatableLabel | translate", (onClick)="onSubmit()")
